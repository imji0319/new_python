<TR ACOPY="Y" ADELETE="Y">

<TH ALIGN="CENTER" VALIGN="MIDDLE" WIDTH="151" HEIGHT="30" ACOPYCOL="Y" ADELETECOL="Y" AMOVECOL="N">
<P>분 류</P>
</TH>

<TH ALIGN="CENTER" VALIGN="MIDDLE" WIDTH="449" HEIGHT="30" ACOPYCOL="Y" ADELETECOL="Y" AMOVECOL="N">
<P>정     의</P>
