<TABLE ACLASS="NORMAL" AFIXTABLE="Y" WIDTH="705" BORDER="0">

<COLGROUP>
<COL WIDTH="696"></COL>
</COLGROUP>

<TBODY>

<TR ACOPY="Y" ADELETE="Y">

<TD AUPDATECONT="N" VALIGN="MIDDLE" HEIGHT="53" WIDTH="705">
<P>&cr;</P>
</TD>
