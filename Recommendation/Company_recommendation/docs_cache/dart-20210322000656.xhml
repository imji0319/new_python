<SPAN USERMARK=" 0X000000">각</SPAN>
<SPAN USERMARK=" 0X000000">각</SPAN>
<SPAN USERMARK=" 0X000000"> </SPAN>
<SPAN USERMARK=" 0X000000">12,224,577</SPAN>
<SPAN USERMARK=" 0X000000">천원과 </SPAN>
